@model IEnumerable<AppDelBagno.Models.Bagno>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>


<p>  
    <div class="form-group">
        
        @if((bool)ViewBag.m_BagnoLibero)
        {
            <input type="submit" value="Bagno libero" class="btn btn-primary" />
        }
        else
        {
            <input type="submit" value="Bagno bloccato" class="btn btn-primary" />
        }
    </div>
</p>


